<div class="modal fade" id="ganadores">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">Lista de ganadores de <span class="inverso">[[ sorteo.nombre ]]</span></h4>
            </div>
            <div class="modal-body">
                <ul>
                    <li ng-repeat="ganador in ganadores">[[ ganador.numero_entrada ]] - <b>[[ ganador.nombre ]] [[ ganador.apellidos ]]</b>, <span class="inverso">[[ ganador.nacionalidad ]]</span>, [[ ganador.universidad ]]</li>
                </ul>
            </div>
            <div class="modal-footer">
                <strong style="float: left">[[ ganadores.length ]] Ganadores</strong>
                <button ng-click="guardarGanadores(ganadores, sorteo)" type="button" class="btn-u btn-estud"><i class="fa fa-save"></i> Guardar ganadores</button>
                <button type="button" class="btn-u btn-brd btn-u-red" data-dismiss="modal"><i class="fa fa-times"></i> Cerrar</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->
</div><!-- /.modal -->